{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
  "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
  "types": ["jest"]
  },
  "include": ["src/app.ts", "src/BasicTrading.ts"],
  "exclude": ["node_modules", "test", "**/*.test.ts"]
}
